{
  "openapi": "3.0.1",
  "info": {
    "title": "TaskFlowTaskServer",
    "version": "1.0"
  },
  "paths": {
    "/api/tasks/create": {
      "post": {
        "tags": [
          "Task"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TaskModel"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/TaskModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/TaskModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/tasks/get-tasks": {
      "get": {
        "tags": [
          "Task"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "TaskModel": {
        "type": "object",
        "properties": {
          "taskName": {
            "type": "string",
            "nullable": true
          },
          "taskAuthor": {
            "type": "string",
            "nullable": true
          },
          "taskAssignee": {
            "type": "string",
            "nullable": true
          },
          "createdDate": {
            "type": "string",
            "nullable": true
          },
          "dueDate": {
            "type": "string",
            "nullable": true
          },
          "taskPriority": {
            "type": "string",
            "nullable": true
          },
          "taskStatus": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}